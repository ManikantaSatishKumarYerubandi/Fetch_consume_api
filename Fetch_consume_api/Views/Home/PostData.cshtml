<h1 style="text-align:center">Post Data from Json Placeholder</h1>

<script>
    function getPostUserData()
    {
        var url = "https://jsonplaceholder.typicode.com/posts";

        fetch(url).then(response=>response.json())
            .then(responseData =>{
                // write code to display the data in table
                alert("Success -got the data");
                console.log(responseData);
            })
            .catch(err =>{
                alert("Error!!!!");
                console.log(err);
            })
    }
</script>