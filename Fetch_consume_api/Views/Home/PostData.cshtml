<h1 style="text-align:center">Post Data from Json Placeholder</h1>

<script>
    function getPostUserData()
    {
        var url = "https://jsonplaceholder.typicode.com/posts";

        fetch(url).then(response=>response.json())
            .then(responseData =>{
                // write code to display the data in table
                //alert("Success -got the data");
                //console.log(responseData);
                for (var i=0;i<responseData.length;i++)
                {
                    $("table").append("<tr>"+
                    "<td>"+responseData[i].id+"</td>" +
                     "<td>" + responseData[i].userId + "</td>" +
                        "<td>" + responseData[i].title + "</td>" +
                        "<td>" + responseData[i].body + "</td>" +


                    +"</tr>")

                }
            })
            .catch(err =>{
                alert("Error!!!!");
                console.log(err);
            })
    }
</script>

<button class="btn btn-primary" onclick="getPostUserData()">Show Post</button>

<table class="table table-bordered"></table>